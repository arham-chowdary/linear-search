
#include <stdio.h>

int main() {
   int a[20],i,n,data,flag=0;
   printf("Enter value of n : ");
   scanf("%d",&n);
   printf("Enter the values");
   for(i=0;i<n;i++){
       scanf("%d",&a[i]);
   }
   printf("Enter the value to be searched :");
   scanf("%d",&data);
   for(i=0;i<n;i++){
       if(a[i]==data){
           flag=1;
           break;
       }}
           if(flag=1){
           printf("Element found at position %d",i);
       }else{
           printf("Element not available");
       }
  

    return 0;
}
